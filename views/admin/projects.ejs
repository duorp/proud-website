<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>Admin – Projects</title>
</head>
<body>

  <h1>Projects</h1>

  <div class="admin-projects">
    <% projects.forEach(p => { %>
      <div class="admin-projects__item">
        <strong><%= p.title %></strong>
        <% if (p.slug) { %>
          — <a href="/admin/projects/<%= p.slug %>/edit">Edit</a>
        <% } %>
        </div>
    <% }) %>
    </div>

</body>
</html>
